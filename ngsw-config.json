{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"assets/icon/favicon.png",
					"/index.html",
					"/*.css",
					"/*.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/fonts/**",
					"/assets/img/**"
				]
			}
		}
	]
}